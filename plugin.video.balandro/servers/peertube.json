{
  "id": "peertube",
  "name": "Peertube",

  "active": true,

  "find_videos": {
    "ignore_urls": [],
    "patterns": [
      {
        "pattern": "(http(?:s|)://[^/]+)/videos/(?:watch|embed)/([0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12})",
        "url": "\\1/videos/watch/\\2"
      },
      {
        "pattern": "(http(?:s|)://[^/]+)/download/videos/([0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}.*)",
        "url": "\\1/download/videos/\\2"
      }
    ]
  }

}