{
  "id": "dailymotion",
  "name": "DailyMotion",

  "active": true,

  "find_videos": {
    "ignore_urls": [],
    "patterns": [
      {
        "pattern": "dailymotion.com/(?:video/|swf/(?:video/|)|)(?:embed/video/|)([A-z0-9]+)",
        "url": "http://www.dailymotion.com/embed/video/\\1"
      }
    ]
  }

}